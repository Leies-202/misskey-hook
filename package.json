{
  "name": "misskey-hook",
  "version": "1.0.0",
  "description": "Incoming webhook for misskey",
  "author": "Tosuke <tasukeprg@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt generate",
    "serve": "run-s build serve:firebase",
    "serve:firebase": "firebase serve --only hosting,functions",
    "deploy": "run-s build deploy:firebase",
    "deploy:firebase": "firebase deploy"
  },
  "lint-staged": {
    "*.{js,vue,json}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.{ts,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "precommit": "run-s lint-staged"
    }
  },
  "dependencies": {
    "@firebase/app": "^0.3.3",
    "@firebase/auth": "^0.7.1",
    "@firebase/firestore": "^0.6.0",
    "@firebase/functions": "^0.3.0",
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/dotenv": "^1.1.1",
    "firebase": "^5.3.0-1",
    "nuxt-edge": "^2.0.0-25550715.56db988"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.3.2",
    "@tosuke/prettier-config": "^1.0.1",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0"
  },
  "engines": {
    "node": "^8"
  }
}
